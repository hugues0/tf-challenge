{
  "name": "tf-challenge",
  "version": "1.0.0",
  "description": "Task force Challenge",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --exec babel-node ./src/index.js",
    "start:app": "npm run db-migrate && babel-node ./src/index.js",
    "test": "npm run migrate &&  nyc --reporter=html --reporter=text mocha --require @babel/register --timeout 3000 ./src/tests/main.test.js --exit",
    "build": "babel index.js â€” out-file index-compiled.js",
    "db-migrate": "npx sequelize db:migrate",
    "demigrate": "npx sequelize-cli db:migrate:undo:all",
    "migrate": "npx sequelize-cli db:migrate:undo:all && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
    "undo-seeds": "npx sequelize-cli db:seed:undo:all",
    "seeds": "npx sequelize-cli db:seed:all"
  },
  "repository": "https://github.com/hugues0/tf-challenge.git",
  "author": "NTWARI Hugues",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.15.8",
    "@babel/node": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/register": "^7.15.3",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "fs": "^0.0.1-security",
    "joi": "13.1.0",
    "jsonwebtoken": "^8.5.1",
    "mailgen": "^2.0.15",
    "morgan": "^1.10.0",
    "nodemailer": "^6.7.0",
    "nodemon": "^2.0.13",
    "path": "^0.12.7",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.7.0",
    "sequelize-cli": "^6.2.0",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.5"
  },
  "devDependencies": {
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "coveralls": "^3.1.1",
    "mocha": "^9.1.3",
    "mocha-lcov-reporter": "^1.3.0",
    "mock-req-res": "^1.2.0",
    "node-mocks-http": "^1.11.0",
    "nyc": "^15.1.0",
    "should": "^13.2.3",
    "sinon": "^11.1.2",
    "sinon-chai": "^3.7.0",
    "sinon-express-mock": "^2.2.1"
  }
}
